@model IEnumerable<BENewTopZone.Models.Category>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Quản Lý Loại Thiết Bị</h2>


<table class="table table-striped">
    <tr style="background-color: aqua">
        <th>
            Tên Loại Thiết Bị
        </th>
        <th>Thao Tác</th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NameCate)
            </td>
            <td>
                @Html.ActionLink("Sửa", "Edit", new { id = item.CatID }, new { @class = "btn btn-success" })|
                @Html.ActionLink("Xóa", "Delete", new { id = item.CatID }, new { @class = "btn btn-danger" }) 

                @*@Html.ActionLink("Chi Tiết", "Details", new { id = item.CatID }, new { @class = "btn btn-danger" }) |*@
            </td>
        </tr>
    }
</table>
<p>
    @Html.ActionLink("Thêm mới", "Create", null, new { @class = "btn btn-primary" })
</p>
